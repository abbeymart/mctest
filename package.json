{
  "name": "@mconnect/mctest",
  "version": "0.2.4",
  "description": "mConnect Testing Package",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/"
  ],
  "scripts": {
    "clean": "rm -rf lib",
    "build-watch": "tsc -w",
    "build": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "prepublishOnly": "npm run build",
    "test": "mocha --timeout 5000 --recursive --reporter mochawesome",
    "dev": "nodemon dist/start.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abbeymart/mctest.git"
  },
  "keywords": [
    "testing",
    "test cases",
    "test scenarios",
    "unit testing",
    "regression testing",
    "integration testing"
  ],
  "author": "Abi Akindele [abbeymart | mconnect.biz]",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abbeymart/mctest/issues"
  },
  "homepage": "https://github.com/abbeymart/mctest#readme",
  "devDependencies": {
    "@types/node": "^14.14.13",
    "typescript": "^4.1.3"
  }
}
